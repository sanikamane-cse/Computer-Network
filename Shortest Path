#include <iostream>
using namespace std;

#define MAX 10
#define INF 10000

int distMat[MAX][MAX];
int distArr[MAX];
bool visited[MAX];
int parent[MAX];

void dijkstra(int n, int src) {
    for (int i = 0; i < n; i++) {
        distArr[i] = distMat[src][i];
        visited[i] = false;
        parent[i] = src;
    }
    visited[src] = true;

    for (int count = 1; count < n; count++) {
        int minDist = INF, u = -1;

        for (int i = 0; i < n; i++) {
            if (!visited[i] && distArr[i] < minDist) {
                minDist = distArr[i];
                u = i;
            }
        }

        visited[u] = true;

        for (int v = 0; v < n; v++) {
            if (!visited[v] && distArr[u] + distMat[u][v] < distArr[v]) {
                distArr[v] = distArr[u] + distMat[u][v];
                parent[v] = u;
            }
        }
    }
}

int main() {
    int n, src, dest;

    cout << "Enter number of nodes : ";
    cin >> n;

    cout << "\nEnter Distance Matrix:\n";
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            cout << "Distance between " << i << " and " << j << " : ";
            cin >> distMat[i][j];

            if (distMat[i][j] == 0 && i != j)
                distMat[i][j] = INF;
        }
    }

    cout << "\nEnter source node : ";
    cin >> src;

    cout << "Enter destination node : ";
    cin >> dest;

    dijkstra(n, src);

    cout << "\nDistance of shortest path is : " << distArr[dest] << endl;

    return 0;
}
